{
  "name": "rentalx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "create-network": "docker network create rentalx-network",
    "up": "docker-compose up --no-recreate",
    "stop": "docker-compose stop",
    "prune": "docker system prune",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration-create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/shared/infra/typeorm/migrations/",
    "migration-run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./src/shared/infra/typeorm/index.ts",
    "migration-revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:revert  -d ./src/shared/infra/typeorm/index.ts",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed-admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.418.0",
    "aws-sdk": "^2.1464.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "csv-parse": "^5.4.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.418.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
